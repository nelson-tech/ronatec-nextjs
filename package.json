{
  "name": "nextjs",
  "version": "0.2.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "dev": "next dev -p 8140",
    "build": "next build",
    "start": "next start -p 8140",
    "lint": "next lint",
    "pm2-dev": "pm2 start npm --name \"Ronatec Next.js Dev\" -- run dev",
    "pm2-dev-stop": "pm2 stop \"Ronatec Next.js Dev\" && pm2 delete \"Ronatec Next.js Dev\"",
    "pm2-dev-restart": "pm2 restart \"Ronatec Next.js Dev\"",
    "pm2-start": "pm2 start npm --name \"Ronatec Next.js Production\" -- run start",
    "pm2-stop": "pm2 stop \"Ronatec Next.js Production\" && pm2 delete \"Ronatec Next.js Production\"",
    "pm2-restart": "pm2 restart \"Ronatec Next.js Production\"",
    "build:css": "postcss assets/main.css -o assets/main_browser.css",
    "codegen": "graphql-codegen -r dotenv/config -c lib/api/codegen/config.ts -w ",
    "schema": "graphql-codegen --config codegen.yml",
    "test": "jest --watch --coverage \".*\\.test\\.ts*\"",
    "tests": "jest --coverage \".*\\.test\\.ts*\"",
    "test:e2e": "playwright test"
  },
