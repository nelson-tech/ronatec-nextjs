{
  "name": "nextjs",
  "version": "0.2.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "dev": "next dev -p 8140",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pm2-dev-start": "pm2 start npm --name \"Ronatec Next.js Dev\" -- run dev",
    "pm2-dev-stop": "pm2 stop \"Ronatec Next.js Dev\" && pm2 delete \"Ronatec Next.js Dev\"",
    "pm2-dev-restart": "pm2 restart \"Ronatec Next.js Dev\"",
    "pm2-stage-start": "PORT=8240 pm2 start npm --name \"Ronatec Next.js Staging\" -- run start",
    "pm2-stage-stop": "pm2 stop \"Ronatec Next.js Staging\" && pm2 delete \"Ronatec Next.js Staging\"",
    "pm2-stage-restart": "pm2 restart \"Ronatec Next.js Staging\"",
    "pm2-prod-start": "PORT=8340 pm2 start npm --name \"Ronatec Next.js Production\" -- run start",
    "pm2-prod-stop": "pm2 stop \"Ronatec Next.js Production\" && pm2 delete \"Ronatec Next.js Production\"",
    "pm2-prod-restart": "pm2 restart \"Ronatec Next.js Production\"",
    "git-update": "git pull && yarn install && yarn build",
    "dev-update": "git checkout dev && yarn git-update && yarn pm2-dev-stop; yarn pm2-dev-start",
    "staging-update": "git checkout staging && yarn git-update && yarn pm2-stage-stop; yarn pm2-stage-start",
    "production-update": "git checkout main && yarn git-update && yarn pm2-prod-stop; yarn pm2-prod-start",
    "build:css": "postcss assets/main.css -o assets/main_browser.css",
    "codegen": "graphql-codegen -r dotenv/config -c lib/api/codegen/config.ts -w ",
    "schema": "graphql-codegen --config codegen.yml",
    "test": "jest --watch --coverage \".*\\.test\\.ts*\"",
    "tests": "jest --coverage \".*\\.test\\.ts*\"",
    "test:e2e": "playwright test",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.1.0",
    "cookies-next": "^2.1.1",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "graphql-tag": "^2.12.6",
    "html-react-parser": "^3.0.16",
    "jwt-decode": "^3.1.2",
    "jwt-encode": "^1.0.1",
    "lodash.debounce": "^4.0.8",
    "next": "^13.3.4",
    "next-seo": "^6.0.0",
    "nodemailer": "^6.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-player": "^2.12.0",
    "react-simple-maps": "^3.0.0",
    "react-slick": "^0.29.0",
    "sharp": "^0.32.1",
    "slick-carousel": "^1.8.1",
    "smoothscroll-polyfill": "^0.4.4",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@next/bundle-analyzer": "^13.3.4",
    "@playwright/test": "^1.33.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/jwt-decode": "^3.1.0",
    "@types/jwt-encode": "^1.0.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.16.3",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemailer-smtp-transport": "^2.7.5",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.0",
    "@types/react-simple-maps": "^3.0.0",
    "@types/react-slick": "^0.23.10",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "autoprefixer": "^10.4.14",
    "encoding": "^0.1.13",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.4",
    "eslint-config-prettier": "^8.8.0",
    "husky": ">=8",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": ">=13",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,css,md}": "prettier --write"
  }
}