{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pm2-dev": "pm2 start npm --name \"Ronatec Next.js Dev\" -- run dev",
    "pm2-dev-stop": "pm2 stop \"Ronatec Next.js Dev\" && pm2 delete \"Ronatec Next.js Dev\"",
    "pm2-dev-restart": "pm2 restart \"Ronatec Next.js Dev\"",
    "pm2-start": "pm2 start npm --name \"Ronatec Next.js Production\" -- run start",
    "pm2-stop": "pm2 stop \"Ronatec Next.js Production\" && pm2 delete \"Ronatec Next.js Production\"",
    "pm2-restart": "pm2 restart \"Ronatec Next.js Production\"",
    "build:css": "postcss assets/main.css -o assets/main_browser.css",
    "schema-gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@react-google-maps/api": "^2.7.0",
    "apollo3-cache-persist": "^0.13.0",
    "classnames": "^2.3.1",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "html-react-parser": "^1.4.4",
    "isomorphic-unfetch": "^3.1.0",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "keen-slider": "^6.6.3",
    "lodash.isequal": "^4.5.0",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-player": "^2.9.0",
    "react-use": "^17.3.2",
    "smoothscroll-polyfill": "^0.4.4",
    "swr": "^1.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@emotion/babel-plugin": "^11.7.2",
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/fragment-matcher": "3.2.1",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.4",
    "@next/bundle-analyzer": "^12.0.8",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@types/js-cookie": "^3.0.1",
    "@types/jwt-decode": "^3.1.0",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^17.0.5",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.38",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^8.7.0",
    "eslint-config-next": "12.0.8",
    "postcss": "^8.4.5",
    "postcss-nesting": "^10.1.2",
