{
  "name": "nextjs",
  "version": "0.2.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pm2-dev": "pm2 start npm --name \"Ronatec Next.js Dev\" -- run dev",
    "pm2-dev-stop": "pm2 stop \"Ronatec Next.js Dev\" && pm2 delete \"Ronatec Next.js Dev\"",
    "pm2-dev-restart": "pm2 restart \"Ronatec Next.js Dev\"",
    "pm2-start": "pm2 start npm --name \"Ronatec Next.js Production\" -- run start",
    "pm2-stop": "pm2 stop \"Ronatec Next.js Production\" && pm2 delete \"Ronatec Next.js Production\"",
    "pm2-restart": "pm2 restart \"Ronatec Next.js Production\"",
    "build:css": "postcss assets/main.css -o assets/main_browser.css",
    "schema": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@headlessui/react": "^1.6.2",
    "@heroicons/react": "^1.0.6",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.8.10",
    "@react-google-maps/api": "^2.11.2",
    "@urql/exchange-auth": "^0.1.7",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "html-react-parser": "^1.4.12",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "keen-slider": "^6.6.14",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "next": "12.1.6",
    "next-seo": "^5.4.0",
    "next-urql": "^3.3.2",
    "nodemailer": "^6.7.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-headroom": "^3.2.0",
    "react-hook-form": "^7.31.1",
    "react-player": "^2.10.1",
    "sharp": "^0.30.4",
    "smoothscroll-polyfill": "^0.4.4",
    "urql": "^2.2.0",
    "zod": "^3.17.3",
    "zustand": "^4.0.0-rc.1"
